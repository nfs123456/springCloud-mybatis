<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nfs.mapper.CourseStockMapper">
    <update id="updateStock" parameterType="com.nfs.bean.Course">
        update tb_course set stock_num = #{stock_num} where id = #{id}
    </update>
</mapper>